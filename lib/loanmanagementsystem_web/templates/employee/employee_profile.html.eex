	<!-- Row -->

    	<!-- Main Content-->
			<div class="main-content pt-0">

				<div class="container">
					<div class="col-md-12">

                     <%= for current_user_detail <- @current_user_details do%>
						<div class="row row-sm">
							<div class="col-xl-3 col-lg-12 col-md-12">
