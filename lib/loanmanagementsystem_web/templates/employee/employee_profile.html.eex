																<p>Name: <%= @companys && @companys.companyName %> </p>
																<p>Email: <%= @companys && @companys.contactEmail %> </p>
																<p class="mb-0">Phone Number: <%= @companys && @companys.companyPhone %></p>
															</div>

														</div>
													</div>

												</div>
											</div>
											<div class="tab-pane fade" id="trackorder" role="tabpanel">
