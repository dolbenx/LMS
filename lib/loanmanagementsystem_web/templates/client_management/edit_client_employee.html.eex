                                                                        <input type="date" class="form-control" value="<%= employee.dateOfBirth %>" max="<%= Timex.shift(Timex.today(), years: -18) %>" name="dateOfBirth" readonly>
                                                                    </div>
                                                                    <span class="form-text text-muted">Please Select date of birth.</span>
                                                                </div>
                                                                <div class="fv-plugins-message-container"></div>
                                                            </div>
                                                            <div class="form-group row fv-plugins-icon-container has-success">
