                                                                <div class="form-group row fv-plugins-icon-container has-success">
                                                                    <label class="col-3 col-form-label">Account Number: <span style="color: red;">*</span></label>
                                                                    <div class="col-9">
                                                                        <div class="input-group input-group-sm">
                                                                            <input type="number" class="form-control" value="<%= employee.accountNumber %>" name="bank_account_number">
                                                                        </div>
                                                                        <span class="form-text text-muted">Please enter account number.</span>
                                                                    </div>
                                                                    <div class="fv-plugins-message-container"></div>
                                                                </div>
                                                                <div class="form-group row fv-plugins-icon-container has-success">
                                                                    <label class="col-3 col-form-label">Account Name: </label>
                                                                    <div class="col-9">
                                                                        <div class="input-group input-group-sm">
                                                                            <input type="text" class="form-control" value="<%= employee.accountName %>" name="accountName">
                                                                        </div>
                                                                        <span class="form-text text-muted">Please enter account name.</span>
                                                                    </div>
                                                                    <div class="fv-plugins-message-container"></div>
                                                                </div>
                                                            </div>
                                                        </div>
                                                        <div class="col-md-6 border">
                                                            <br><h5 class="text-center"> <u>Income Details</u> </h5><br>
                                                            <div class="form-group row fv-plugins-icon-container has-success">
                                                                <label class="col-3 col-form-label">Gross Salary (ZMW):</label>
                                                                <div class="col-9">
                                                                    <div class="input-group input-group-sm">
                                                                        <input type="number" class="form-control" name="gross_pay" value="<%= :erlang.float_to_binary((employee.gross_pay), [{:decimals, 2}]) %>" id="gross_pay" oninput="calculate()">
                                                                    </div>
                                                                    <span class="form-text text-muted">Please employee gross pay.</span>
                                                                </div>
                                                                <div class="fv-plugins-message-container"></div>
                                                            </div>

                                                            <div class="form-group row fv-plugins-icon-container has-success">
                                                                <label class="col-3 col-form-label">Total Deductions (ZMW):</label>
                                                                <div class="col-9">
                                                                    <div class="input-group input-group-sm">
                                                                        <input type="number" class="form-control" name="total_deductions" value="<%= :erlang.float_to_binary((employee.total_deductions), [{:decimals, 2}]) %>" id="total_deductions" oninput="calculate()">
                                                                    </div>
                                                                    <span class="form-text text-muted">Please enter employee total deductions.</span>
                                                                </div>
                                                                <div class="fv-plugins-message-container"></div>
                                                            </div>
                                                            <div class="form-group row fv-plugins-icon-container has-success" >
                                                                <label class="col-3 col-form-label">Net Pay (ZMW):</label>
                                                                <div class="col-9" >
                                                                    <div class="input-group input-group-sm" >
                                                                        <input type="text" class="form-control" id="net_pay" value="<%= :erlang.float_to_binary((employee.net_pay), [{:decimals, 2}]) %>" name="net_pay" readonly>
                                                                    </div>
                                                                    <span class="form-text text-muted">Please enter employee net pay.</span>
                                                                </div>
                                                                <div class="fv-plugins-message-container"></div>
                                                            </div>


                                                            <div class="form-group row fv-plugins-icon-container has-success">
                                                                <label class="col-3 col-form-label">Loan Limit:</label>
                                                                <div class="col-9">
                                                                    <div class="input-group input-group-sm">
                                                                        <input type="text" class="form-control" value="<%= :erlang.float_to_binary((employee.limit), [{:decimals, 2}]) %>" id="loan_limit_amount" name="loan_limit" readonly>
                                                                    </div>
                                                                    <span class="form-text text-muted">Please enter employee loan limit.</span>
                                                                </div>
                                                                <div class="fv-plugins-message-container"></div>
                                                            </div>

                                                            <div class="form-group row fv-plugins-icon-container has-success">
                                                                <label class="col-3 col-form-label">Pay Day:</label>
