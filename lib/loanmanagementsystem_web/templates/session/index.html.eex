<html lang="en">
	<!--begin::Head-->
	<head><base href="../../../../">
		<meta charset="utf-8" />
		<title>Pangea | Login</title>
		<meta name="description" content="Login page example" />
		<meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no" />
		<link rel="canonical" href="https://keenthemes.com/metronic" />
		<!--begin::Fonts-->
		<link rel="stylesheet" href="https://fonts.googleapis.com/css?family=Poppins:300,400,500,600,700" />
		<!--end::Fonts-->
		<!--begin::Page Custom Styles(used by this page)-->
		<link href="/webstatic/css/pages/login/classic/login-5.css" rel="stylesheet" type="text/css" />
		<!--end::Page Custom Styles-->
		<!--begin::Global Theme Styles(used by all pages)-->
		<link href="/webstatic/plugins/global/plugins.bundle.css" rel="stylesheet" type="text/css" />
		<link href="/webstatic/plugins/custom/prismjs/prismjs.bundle.css" rel="stylesheet" type="text/css" />
		<link href="/webstatic/css/style.bundle.css" rel="stylesheet" type="text/css" />
		<!--end::Global Theme Styles-->
		<!--begin::Layout Themes(used by all pages)-->
		<link href="/webstatic/css/themes/layout/header/base/light.css" rel="stylesheet" type="text/css" />
		<link href="/webstatic/css/themes/layout/header/menu/light.css" rel="stylesheet" type="text/css" />
		<link href="/webstatic/css/themes/layout/brand/dark.css" rel="stylesheet" type="text/css" />
		<link href="/webstatic/css/themes/layout/aside/dark.css" rel="stylesheet" type="text/css" />
		<!--end::Layout Themes-->
		<link rel="shortcut icon" href="<%= Routes.static_path(@conn, "/images/add.png") %>" />

		<head>
	<style>
		.my_div{
		border: 1px solid black;
		padding: 25px 50px 75px 100px;
		background-color: lightblue;
		}
	</style>
</head>

	</head>
	<!--end::Head-->
	<!--begin::Body-->
	<body id="kt_body" class="header-fixed header-mobile-fixed subheader-enabled subheader-fixed aside-enabled aside-fixed aside-minimize-hoverable page-loading col-md-12" style="background-color: #24BFCD; height: 100%; background-position: center; background-repeat: no-repeat; background-size: cover;">
		<!--begin::Main-->
		<div class="d-flex flex-column flex-root">
			<div class="row" style="padding-top: 50px;">
				<div class="col-md-6" style="padding-left: 200px;">
					<div class="container">

					</div>
				</div>
				<div class="col-md-6 text-center"></div>
			</div>
			<!--begin::Login-->
			<div class="login login-5 login-signin-on d-flex flex-row-fluid" id="kt_login">
				<div class="d-flex flex-center flex-row-fluid" >
					<div class="col-md-6" style="color: white;">
					<div class="col-md-6 col-lg-11" style="background-color: black; color: white; border-radius:10px; font-size: 38px; text-align: center; font-family: sans-serif;"><strong>Pangaea</strong>&nbsp;<img alt="Logo" src="<%= Routes.static_path(@conn, "/images/add.png") %>" style="height: 50px;"/>&nbsp;<strong>Capital</strong></div>
							<h3 style="font-size: 30px; color: black; text-align: justify;" class="col-lg-11"> Welcome to Pangaea ADD Capital LMS.</h3><br>
							<h4 style="line-height:1.6em; color: black; text-align: justify;" class="col-lg-11">As Pangaea ADD Capital, we pride ourselves as ’The Innovative Supply Chain Finance Specialist’.
                     			Our core offering are Invoice Discounting and Selective Order Finance, for Prevetted Off Takers / Anchors
								In addition to providing funding we offer financial consultancy and provide technical assistance to our clients.
								At Pangaea ADD Capital we view our clients as partners. We care for their business and are invested in seeing our partners grow and develop.<h4>
					</div>
					<div class="login-form position-relative overflow-hidden col-md-6 card card-custom" style="border-radius:25px;">
					<div class="container">

						<div class="row">
							<div class="col-md-12" style="padding-top: 10px;">
								<div class="form-group">
									<%= if get_flash(@conn, :info) do %>
									<div class="alert alert-success  alert-styled-left" style="border-radius:15px;" role="alert">
									<button type="button" class="close" data-dismiss="alert">&times; <span class="sr-only">Close</span></button>
									<%= get_flash(@conn, :info) %>
									</div>
									<% end %>
									<%= if get_flash(@conn, :error) do %>
									<div class="alert alert-danger alert-styled-left" style="border-radius:15px;">
									<button type="button" class="close" data-dismiss="alert">&times; <span class="sr-only">Close</span></button>
									Sorry! <%= get_flash(@conn, :error) %>
									</div>
									<% end %>
								</div>
							</div>
						</div>
							<!--begin::Login Header-->
							<!--<div class="d-flex flex-center mb-15">
								<a href="#">
									<img src="<%= Routes.static_path(@conn, "/images/imgingest.png") %>" class="max-h-75px" alt="" style="border-radius: 50%; width:80px; height:170px;"/>
								</a>
							</div>-->
							<!--end::Login Header-->
							<!--begin::Login Sign in form-->
							<div class="login-signin mb-30">
								<div class="text-center" style="padding-top: 30px; padding-bottom: 30px; color: #14314F;">
									<h3 class="opacity-100 font-weight-normal">Sign In <i style="color: #24BFCD;" class="flaticon-user icon-xl"></i></h3>
									<p class="opacity-50">Enter your details to login to your account:</p>
								</div>
								<form class="form" action="<%= Routes.session_path(@conn, :create)%>" method="POST">
									<div class="form-group">
										<label>Username </label>
										<input class="form-control rounded-pill py-4 px-8" type="text" name="username" required/>
									</div>
									<div class="form-group">
										<label>Password </label>
										<input class="form-control rounded-pill py-4 px-8" type="password" name="password" required/>
									</div>
									<div class="form-group d-flex flex-wrap justify-content-between align-items-center px-8 opacity-60">
										<div class="checkbox-inline" style="color: #14314F;">
											<label class="checkbox checkbox-outline m-0">
											<input type="checkbox" name="remember" />
											<span></span>Remember me</label>
										</div>
										<a href="/forgot-password" id="kt_login_forgot" class="font-weight-bold" style="color: #14314F;">Forget Password ?</a>
									</div>
									<div class="form-group text-center">
										<button class="btn btn-pill opacity-90 px-40" style="background-color: #00A2B4; color: white;">Sign In</button>
									</div>
								</form>
								<div class="mt-10">
								</div>
							</div>
							<!--end::Login Sign in form-->

						<!--begin::Forgot-->
						<div class="login-form login-forgot">
							<div class="text-center mb-10 mb-lg-20">
								<h3 class="font-size-h1">Forgotten Password ?</h3>
								<p class="text-muted font-weight-bold">Enter your email to reset your password</p>
							</div>
							<!--begin::Form-->
							<form class="form" novalidate="novalidate" id="kt_login_forgot_form">
								<div class="form-group">
									<input class="form-control form-control-solid h-auto py-5 px-6" type="email" placeholder="Email" name="email" autocomplete="off" />
								</div>
								<div class="form-group d-flex flex-wrap flex-center">
									<button type="button" id="kt_login_forgot_submit" class="btn btn-primary font-weight-bold px-9 py-4 my-3 mx-4">Submit</button>
									<button type="button" id="kt_login_forgot_cancel" class="btn btn-light-primary font-weight-bold px-9 py-4 my-3 mx-4">Cancel</button>
								</div>
							</form>
							<!--end::Form-->
						</div>
						<!--end::Forgot-->
						</div>
					</div>
				</div>
			</div>
			<!--end::Login-->
		</div>

		<div class="row">
			<div class="container text-center text-black"  style="padding-bottom: 50px;">
				Powered By &nbsp;&nbsp;<img alt="ProbaseLogo" src="<%= Routes.static_path(@conn, "/images/probaselogo.png") %>"  style="height: 40px;"/>
			</div>
		</div>
		<!--end::Main-->
		<script>var HOST_URL = "https://preview.keenthemes.com/metronic/theme/html/tools/preview";</script>
		<!--begin::Global Config(global config for global JS scripts)-->
		<script>var KTAppSettings = { "breakpoints": { "sm": 576, "md": 768, "lg": 992, "xl": 1200, "xxl": 1400 }, "colors": { "theme": { "base": { "white": "#ffffff", "primary": "#3699FF", "secondary": "#E5EAEE", "success": "#1BC5BD", "info": "#8950FC", "warning": "#FFA800", "danger": "#F64E60", "light": "#E4E6EF", "dark": "#181C32" }, "light": { "white": "#ffffff", "primary": "#E1F0FF", "secondary": "#EBEDF3", "success": "#C9F7F5", "info": "#EEE5FF", "warning": "#FFF4DE", "danger": "#FFE2E5", "light": "#F3F6F9", "dark": "#D6D6E0" }, "inverse": { "white": "#ffffff", "primary": "#ffffff", "secondary": "#3F4254", "success": "#ffffff", "info": "#ffffff", "warning": "#ffffff", "danger": "#ffffff", "light": "#464E5F", "dark": "#ffffff" } }, "gray": { "gray-100": "#F3F6F9", "gray-200": "#EBEDF3", "gray-300": "#E4E6EF", "gray-400": "#D1D3E0", "gray-500": "#B5B5C3", "gray-600": "#7E8299", "gray-700": "#5E6278", "gray-800": "#3F4254", "gray-900": "#181C32" } }, "font-family": "Poppins" };</script>
		<!--end::Global Config-->
		<!--begin::Global Theme Bundle(used by all pages)-->
		<script src="/webstatic/plugins/global/plugins.bundle.js"></script>
		<script src="/webstatic/plugins/custom/prismjs/prismjs.bundle.js"></script>
		<script src="/webstatic/js/scripts.bundle.js"></script>
		<!--end::Global Theme Bundle-->
		<!--begin::Page Scripts(used by this page)-->
		<script src="/webstatic/js/pages/custom/login/login-general.js"></script>
		<!--end::Page Scripts-->
	</body>
	<!--end::Body-->
</html>
