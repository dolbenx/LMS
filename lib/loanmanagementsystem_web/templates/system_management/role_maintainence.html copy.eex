

<div id="kt_header" class="header header-fixed">
        <!--begin::Container-->
        <div class="container-fluid d-flex align-items-stretch justify-content-between">
            <!--begin::Header Menu Wrapper-->
                <%= render LoanmanagementsystemWeb.OrganizationManagementView, "submenu.html", assigns %>
            <!--end::Header Menu Wrapper-->
        </div>
        <!--end::Container-->
    </div>
    <!--end::Header-->
    <%= render LoanmanagementsystemWeb.UserView, "aleart.html", assigns %>
    <!--begin::Content-->
    <div class="content d-flex flex-column flex-column-fluid" id="kt_content">
        <!--begin::Subheader-->
        <div class="subheader py-2 py-lg-4 subheader-solid" id="kt_subheader">
            <div class="container-fluid d-flex align-items-center justify-content-between flex-wrap flex-sm-nowrap">
                <!--begin::Info-->
                <div class="d-flex align-items-center flex-wrap mr-2">
                    <!--begin::Page Title-->
                    <h5 class="text-dark font-weight-bold mt-2 mb-2 mr-5">Role Maintainence</h5>
                    <!--end::Page Title-->
                    <!--begin::Actions-->
                    <div class="subheader-separator subheader-separator-ver mt-2 mb-2 mr-4 bg-gray-200"></div>
                    <!--end::Actions-->
                </div>
                <!--end::Info-->
                <!--begin::Toolbar-->
                <div class="d-flex align-items-center">
                    <!--begin::Daterange-->
                    TIME
                </div>
                <!--end::Toolbar-->
            </div>
        </div>
        <!--end::Subheader-->
        <!--begin::Entry-->
        <div class="d-flex flex-column-fluid">
            <!--begin::Container-->
            <div class="container-fluid">
                <!--begin::Dashboard-->
                <!--begin::Row-->
                <div class="row">
                    <div class="col-md-12 col-xxl-12">
                        <div class="card card-custom card-stretch gutter-b">
                            <div class="card-header flex-wrap border-0 pt-6 pb-0">
                                <div class="card-title">
                                    <h3 class="card-label">Role Maintainence List
                                    <span class="text-muted pt-2 font-size-sm d-block">List of role maintainence</span></h3>
                                </div>
                                <div class="card-toolbar">
                                    <%= if Loanmanagementsystem.Accounts.User.has_role?(@user, :user_role_maintenance, :create) do %>
                                        <button type="button" class="btn ripple text-white font-weight-bolder" data-toggle="modal" data-target="#add_user_role" style="background-color: #011941;">
                                            Add Role
                                        </button>
                                    <% end %>
                                </div>
                            </div>
                            <div class="card-body">
                                <table class="table table-bordered table-hover table-striped w-100" id="kt_datatable">
                                <input type="hidden" name="_csrf_token" id="csrf" value="<%= Plug.CSRFProtection.get_csrf_token()%>">
                                    <thead>
                                        <tr>
                                            <th>Role Group</th>
                                            <th>Role Name</th>
                                            <th class="text-center">Status</th>
                                            <th class="text-center">Action</th>
                                        </tr>
                                    </thead>

                                    <tbody>
                                        <%= for user_role <- @user_roles do %>
                                        <tr>
                                            <td>
                                                <%= case String.contains?(user_role.role_group, "_") do
                                                    true -> String.replace(user_role.role_group, "_", " ")
                                                    false -> user_role.role_group
                                                end %>
                                            </td>
                                            <td>
                                                <%= case String.contains?(user_role.role_desc, "_") do
                                                    true -> String.replace(user_role.role_desc, "_", " ")
                                                    false -> user_role.role_desc
                                                end %>

                                            </td>
                                            <td style="text-align: center; ">
                                                <%= if user_role.status == "ACTIVE" do %>
                                                    <span class="badge bg-pill bg-success"><%= user_role.status %></span>
                                                <% else %>
                                                    <span class="badge bg-pill bg-danger"><%= user_role.status %></span>
                                                <% end %>
                                            </td>
                                            <td class="text-center">
                                                <%= if Loanmanagementsystem.Accounts.User.has_role?(@user, :user_role_maintenance, :edit) do %>
                                                        <a class="btn btn-sm btn-info" href="<%= Routes.system_management_path(@conn, :edit_user_roles, id: user_role.id) %>">
                                                            <i class="flaticon-edit icon-sm"></i>
                                                            Edit
                                                        </a>
                                                <% end %>
                                                        <a class="btn btn-sm btn-warning" href="<%= Routes.system_management_path(@conn, :view_user_roles, id: user_role.id) %>">
                                                            <i class="flaticon-eye icon-sm"></i>
                                                            View
                                                        </a>
                                                <%= if Loanmanagementsystem.Accounts.User.has_role?(@user, :user_role_maintenance, :change_status) do %>

                                                        <!--a class="btn btn-sm btn-success role-change-status" href="#" data-id="<%= user_role.id %>" data-status="ACTIVE" style="display: <%= if user_role.status == "INACTIVE", do: "block;", else: "none;" %>"><i class="flaticon-check"></i> Activate</a>

                                                        <a class="btn btn-sm btn-danger role-change-status" href="#"  data-id="<%= user_role.id %>"  data-status="INACTIVE" style="display: <%= if user_role.status == "ACTIVE", do: "block;", else: "none;" %>"><i class="flaticon-times"></i> Disable</a -->

                                                        <%= if user_role.status =="ACTIVE" do %>

                                                            <a href="#" data-deactivate-id="<%= user_role.id %>" class="role-change-status btn btn-sm btn-danger">
                                                                <i class="flaticon-cancel icon-sm"></i> Disable </a>
                                                            <% else %>
                                                            <a href="#" data-deactivate-id="<%= user_role.id %>" class="role-change-status btn btn-sm btn-success">
                                                                <i class="flaticon-check icon-sm"></i> Enable </a>
                                                        <% end %>

                                                <% end %>
                                            </td>
                                        </tr>
                                        <% end %>
                                    </tbody>
                                </table>
                            </div>
                        </div>
                    </div>
                </div>
                <!--end::Row-->
            </div>
            <!--end::Container-->
        </div>
        <!--end::Entry-->
    </div>

