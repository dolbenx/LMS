                                                                data-phone = "<%=get_bio_data.mobilenumber %>"
                                                                data-meansofidentificationtype = "<%=get_bio_data.meansofidentificationtype %>"
                                                                data-meansofidentificationnumber = "<%=get_bio_data.meansofidentificationnumber %>"
                                                                data-title = "<%=get_bio_data.title %>"
                                                                data-gender = "<%=get_bio_data.gender %>"
                                                            >Enable</a>

                                                            <% else %>

                                                            <a class="btn ripple btn-sm" href="#"
                                                                data-target="#disableusermodal"
                                                                data-toggle="modal"
                                                                data-id="<%= get_bio_data.id %>"
                                                                data-firstname= "<%= get_bio_data.firstname%>"
                                                                data-lastname="<%= get_bio_data.lastname%>"
                                                                data-othername="<%= get_bio_data.othername%>"
                                                                data-email ="<%= get_bio_data.emailaddress%> "
                                                                data-dob = "<%=get_bio_data.dateofbirth %>"
                                                                data-phone = "<%=get_bio_data.mobilenumber %>"
                                                                data-meansofidentificationtype = "<%=get_bio_data.meansofidentificationtype %>"
                                                                data-meansofidentificationnumber = "<%=get_bio_data.meansofidentificationnumber %>"
                                                                data-title = "<%=get_bio_data.title %>"
                                                                data-gender = "<%=get_bio_data.gender %>"
                                                            >Disable</a>
                                                        <% end %>

                                                        <%= if Loanmanagementsystem.Accounts.User.has_role?(@user, :user, :edit) do %>

                                                            <a class="btn ripple btn-sm" href="#"
                                                                data-target="#edituseradmin"
                                                                data-toggle="modal"
                                                                data-id ="<%= get_bio_data.id %>"
                                                                data-firstname= "<%= get_bio_data.firstname%>"
                                                                data-lastname="<%= get_bio_data.lastname%>"
                                                                data-othername="<%= get_bio_data.othername%>"
                                                                data-email ="<%= get_bio_data.emailaddress%> "
                                                                data-dob = "<%=get_bio_data.dateofbirth %>"
                                                                data-phone = "<%=get_bio_data.mobilenumber %>"
                                                                data-meansofidentificationtype = "<%=get_bio_data.meansofidentificationtype %>"
                                                                data-meansofidentificationnumber = "<%=get_bio_data.meansofidentificationnumber %>"
                                                                data-title = "<%=get_bio_data.title %>"
                                                                data-gender = "<%=get_bio_data.gender %>"
                                                                data-funder_type = "<%=get_bio_data.funder_type %>"
                                                            >Edit</a>
                                                        <% end %>

                                                        <a class="btn ripple btn-sm" href="#"
                                                            data-target="#viewuseradmin"
                                                            data-toggle="modal"
                                                            data-id ="<%= get_bio_data.id %>"
                                                            data-firstname= "<%= get_bio_data.firstname%>"
                                                            data-lastname="<%= get_bio_data.lastname%>"
                                                            data-othername="<%= get_bio_data.othername%>"
                                                            data-email ="<%= get_bio_data.emailaddress%> "
                                                            data-dob = "<%=get_bio_data.dateofbirth %>"
                                                            data-phone = "<%=get_bio_data.mobilenumber %>"
                                                            data-meansofidentificationtype = "<%=get_bio_data.meansofidentificationtype %>"
                                                            data-meansofidentificationnumber = "<%=get_bio_data.meansofidentificationnumber %>"
                                                            data-title = "<%=get_bio_data.title %>"
                                                            data-gender = "<%=get_bio_data.gender %>"
                                                        >View</a>

                                                        <a class="btn ripple btn-sm" href="#"
                                                            data-target="#fund_funder"
                                                            data-toggle="modal"
                                                            data-id ="<%= get_bio_data.id %>"
                                                            data-firstname= "<%= get_bio_data.firstname%>"
                                                            data-lastname="<%= get_bio_data.lastname%>"
                                                            data-othername="<%= get_bio_data.othername%>"
                                                            data-email ="<%= get_bio_data.emailaddress%> "
                                                            data-dob = "<%=get_bio_data.dateofbirth %>"
                                                            data-phone = "<%=get_bio_data.mobilenumber %>"
                                                            data-meansofidentificationtype = "<%=get_bio_data.meansofidentificationtype %>"
                                                            data-meansofidentificationnumber = "<%=get_bio_data.meansofidentificationnumber %>"
                                                            data-title = "<%=get_bio_data.title %>"
                                                            data-gender = "<%=get_bio_data.gender %>"
                                                        >Fund</a>
