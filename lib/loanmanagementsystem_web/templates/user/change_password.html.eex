	<style>
		.russ{
		width: 100px;
		height: 50px;
		position: relative;
		animation-name: example;
		animation-duration: 2s;
		}

		@keyframes example {
		0%   {left:25px; top:0px;}
		25%  {left:200px; top:0px;}

		60% {left:0px; top:0px;}
		}

		.russkay{
		width: 100px;
		height: 100px;
		position: relative;
		animation-name: example;
		animation-duration: 4s;

		}

		@keyframes example {
		0%   {left:25px; top:0px;}
		25%  {left:200px; top:0px;}

		60% {left:0px; top:0px;}
		}

        	.russkay_russkay{

		position: relative;
		animation-name: russk;
		animation-duration: 2s;
		}

		@keyframes russk {
		0%   {right:25px; top:0px;}
		25%  {right:50px; top:0px;}

		60% {right:0px; top:0px;}
		}


	</style>
