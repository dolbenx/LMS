																	<button type="button" class="close" data-dismiss="alert">&times; <span class="sr-only">Close</span></button>
																	Opps! <%= get_flash(@conn, :error) %>
																	</div>
																<% end %>
															</div>
														</div>
													</div>
												<div class="form-group text-start">
													<label>Current Password</label>
													<input type="password" name="user[old_password]" id="lostaccount" class="form-control" required="">
												</div>
												<div class="form-group text-start">
													<label>New Password</label>
													<input type="password" name="user[new_password]" id="lostaccount" class="form-control" required="">
                                                    <div class="invalid-feedback">No, you missed this one.</div>

												</div>
                                                <div class="form-group text-start">
													<label>Confirm New Password</label>
													<input type="password" name="user[new_password]" id="lostaccount" class="form-control" required="">
                                                    <div class="invalid-feedback">No, you missed this one.</div>

												</div>
												<button type="submit" class="btn ripple btn-main-primary btn-block">Change</button>
											</form>
										</div>
								</div>
