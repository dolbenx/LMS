                                                                        <i class="ki ki-bold-more-hor"></i>
                                                                    </button>
                                                                    <div class="dropdown-menu">
                                                                        <%= if Savings.Accounts.User.has_role?(@user, :system_users, :view) do %>
                                                                            <a class="dropdown-item" href="#"
                                                                                data-id="<%= get_bio_data.id %>"
                                                                                data-bio_id="<%= get_bio_data.bio_id %>"
                                                                                data-firstname="<%= get_bio_data.firstname %>"
                                                                                data-lastname="<%= get_bio_data.lastname %>"
                                                                                data-username="<%= get_bio_data.username %>"
                                                                                data-emailaddress="<%= get_bio_data.emailaddress %>"
                                                                                data-meansofidentificationtype="<%= get_bio_data.meansofidentificationtype %>"
                                                                                data-meansofidentificationnumber="<%= get_bio_data.meansofidentificationnumber %>"
                                                                                data-status="<%= get_bio_data.status %>"
                                                                                data-othername="<%= get_bio_data.othername %>"
                                                                                data-title="<%= get_bio_data.title %>"
                                                                                data-dateofbirth="<%= get_bio_data.dateofbirth %>"
                                                                                data-gender="<%= get_bio_data.gender %>"
                                                                                data-roletype="<%= get_bio_data.roletype %>"
                                                                                data-mobilenumber="<%= get_bio_data.mobilenumber %>"
                                                                                data-role_desc="<%= get_bio_data.role_desc %>"
                                                                                data-toggle="modal" data-target="#view_admin"> View</a>
                                                                        <% end %>
                                                                        <%= if Savings.Accounts.User.has_role?(@user, :system_users, :edit) do %>
                                                                            <a class="dropdown-item" href="#"
                                                                            data-id="<%= get_bio_data.id %>"
                                                                            data-bio_id="<%= get_bio_data.bio_id %>"
                                                                            data-firstname="<%= get_bio_data.firstname %>"
                                                                            data-lastname="<%= get_bio_data.lastname %>"
                                                                            data-username="<%= get_bio_data.username %>"
                                                                            data-mobilenumber="<%= get_bio_data.mobilenumber %>"
                                                                            data-emailaddress="<%= get_bio_data.emailaddress %>"
                                                                            data-meansofidentificationtype="<%= get_bio_data.meansofidentificationtype %>"
                                                                            data-meansofidentificationnumber="<%= get_bio_data.meansofidentificationnumber %>"
                                                                            data-status="<%= get_bio_data.status %>"
                                                                            data-othername="<%= get_bio_data.othername %>"
                                                                            data-title="<%= get_bio_data.title %>"
                                                                            data-dateofbirth="<%= get_bio_data.dateofbirth %>"
                                                                            data-gender="<%= get_bio_data.gender %>"
                                                                            data-roletype="<%= get_bio_data.roletype %>"
                                                                            data-role_desc="<%= get_bio_data.role_desc %>"

                                                                            data-toggle="modal" data-target="#edit_admin"> Edit</a>
                                                                        <% end %>
