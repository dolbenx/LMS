                                                                            <%= if get_bio_data.status == "ACTIVE" do %>
                                                                                <a class="dropdown-item js-sweetalert2-deactivate_admin" href="#" data-id="<%= get_bio_data.id %>"> Deactivate</a>
                                                                            <% else %>
                                                                                <a class="dropdown-item js-sweetalert2-activate_admin" href="#" data-id="<%= get_bio_data.id %>"> Activate</a>
                                                                            <% end %>
                                                                        <% end %>
                                                                    </div>
                                                                </div>
                                                                </td>
                                                            </tr>
                                                        <% end %>


												</tbody>
											</table>
										</div>
										<!--end: Datatable-->
